{
  "name": "xbsj-earthui",
  "version": "1.7.14",
  "description": "xbsj-earthui",
  "main": "index.js",
  "keywords": [
    "build",
    "demo",
    "webpack"
  ],
  "author": "xbsj",
  "scripts": {
    "dev-comment": "使用node-modules中的xbsjCesium、xbsjEarth、标绘插件、自定义图元扩展插件进行调试",
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "copyToStatic-comment": "把node-modules中的earthsdk的js库拷贝到Static目录下",
    "copyToStatic": "ncp ./node_modules/earthsdk/dist/XbsjCesium ./Static/XbsjCesium && ncp ./node_modules/earthsdk/dist/XbsjEarth ./Static/XbsjEarth && ncp ./node_modules/earthsdk-plotting-symbol/dist/XbsjEarth-Plugins ./Static/XbsjEarth-Plugins && ncp ./node_modules/earthsdk-custom-primitive/dist/XbsjEarth-Plugins ./Static/XbsjEarth-Plugins",
    "dev2-comment": "使用static目录中的xbsjCesium、xbsjEarth、标绘插件、自定义图元扩展插件进行调试",
    "dev2": "cross-env NODE_ENV=development XBSJ_IMPORT=external webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build2": "cross-env NODE_ENV=production XBSJ_IMPORT=external webpack --progress --hide-modules && npm run postbuild",
    "postbuild": "npm run doc",
    "doc": "jsdoc --configure Tools/jsdoc/conf.json",
    "login": "npm adduser --registry http://registry.npmjs.org",
    "publish": "npm publish --registry http://registry.npmjs.org",
    "gallery-comment": "如果示例发生变化，需要手动执行此命令，已重新生成示例列表",
    "gallery": "node Tools/examples/gallery.js",
    "deprecate": "npm deprecate --registry http://registry.npmjs.org",
    "xbsjDebug-comment": "底层代码调试，只调试xbsjEarth，不调试cesium和xbsjCesium",
    "xbsjDebug": "cross-env NODE_ENV=xbsjDebug webpack-dev-server",
    "xbsjDebug2-comment": "底层代码调试，同时调试cesium、xbsjCesium和xbsjEarth",
    "xbsjDebug2": "cross-env NODE_ENV=xbsjDebug2 webpack-dev-server",
    "xbsjDebug3-comment": "底层代码调试，同时调试cesium、xbsjCesium和xbsjEarth, Cesium使用localhost:8080加载",
    "xbsjDebug3": "cross-env NODE_ENV=xbsjDebug3 webpack-dev-server"
  },
  "dependencies": {
    "cesium": "^1.83.0",
    "echarts": "^4.5.0",
    "iview": "^3.1.3",
    "moment": "^2.24.0",
    "pako": "^1.0.10",
    "vue": "^2.6.10",
    "vue-drag-tree": "^1.1.8",
    "x2js": "^3.3.1",
    "xbsj-renderer": "^0.1.31"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "axios": "^0.21.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "bundle-loader": "^0.5.5",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.11",
    "earthsdk": "^1.7.14",
    "earthsdk-custom-primitive": "1.0.11",
    "earthsdk-plotting-symbol": "^1.0.16",
    "file-loader": "^1.1.6",
    "globby": "^10.0.1",
    "history": "^4.7.2",
    "html-webpack-plugin": "^4.5.2",
    "iview-loader": "^1.3.0",
    "jsdoc": "^3.5.5",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-loader": "^0.6.0",
    "open-browser-webpack-plugin": "0.0.5",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "string-replace-loader": "^2.1.1",
    "style-loader": "^0.19.1",
    "ts-loader": "^4.5.0",
    "typescript": "^3.4.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^14.0.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.8.0"
  }
}
